<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <title>{{ (renderData and renderData.title) or title|default('Coronavirus COVID-19 - Latest daily figures') }}</title>
    <meta name="description" content="{{ (renderData and renderData.description) or description|default('Summary about number of new daily cases for Coronavirus COVID-19') }}" />
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <meta name="msapplication-TileImage" content="{{ '/assets/images/apple-touch-icon-114x114.png' | url }}" />
    <meta name="theme-color" content="#f55555" />
    <link rel="preconnect" href="https://www.googletagmanager.com" />
    <link rel="preconnect" href="https://www.google-analytics.com" />
    <link rel="manifest" href="{{ '/manifest.json' | url }}" />
    <link rel="icon" type="image/x-icon" href="{{ '/assets/images/favicon.ico' | url }}" />
    <link rel="apple-touch-icon" href="{{ '/assets/images/apple-touch-icon-114x114.png' | url }}" />
    <link href="{{ '/assets/css/normalize.css' | url }}" rel="stylesheet" />
    <link href="{{ '/assets/css/styles.css' | url }}" rel="stylesheet" />
  </head>
  <body>
    <div class="wrapper">
      <main class="main">{{ content | safe }}</main>
    </div>
    <script src="/assets/js/main.js"></script>
  </body>
</html>